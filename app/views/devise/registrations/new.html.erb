 <script>
  $(document).ready(function(){
    $("#new_user").validate();
  });
  </script>
<style type="text/css">
      /* Override some defaults */
      html, body {
        background-color: #eee;
      }
      body {
        padding-top: 50px; 
      }
      .container {
        width: 400px;
        padding-top: 90px;
       
      }

      /* The white background content wrapper */
      .container > .content {
        background-color: #fff;
        padding: 20px;
        margin: 0 -20px; 
        -webkit-border-radius: 10px 10px 10px 10px;
           -moz-border-radius: 10px 10px 10px 10px;
                border-radius: 10px 10px 10px 10px;
        -webkit-box-shadow: 0 1px 2px rgba(0,0,0,.15);
           -moz-box-shadow: 0 1px 2px rgba(0,0,0,.15);
                box-shadow: 0 1px 2px rgba(0,0,0,.15);
      }

      .error {
        color: #D8000C;
        font-size: 12px;
        font-style: inherit;
      }

    .login-form {
    margin-left: 65px;
    }
  
    legend {
    margin-right: -50px;
    font-weight: bold;
      color: #404040;
    }

    </style>
<div class="login-form">
    <%= image_tag "logo.png" %> 
  <h2>Sign Up</h2>
<%= form_for(resource, :as => resource_name, :url => registration_path(resource_name)) do |f| %>
  <%= devise_error_messages! %>
<fieldset>
  <div class="control-label">Associate Id</div>
  <div class="controls"><%= f.text_field :associate_id,:class=>"required" %></div>

  <div class="control-label">Associate Name</div>
  <div class="controls"><%= f.text_field :associate_name,:class=>"required" %></div>

  <div class="control-label">Email</div>
  <div class="controls"><%= f.email_field :email, :id => "cemail", :class => "required email" %></div>

  <div class="control-label">Password</div>
  <div class="controls"><%= f.password_field :password,:class=>"required" %></div>

  <div class="control-label">Confirm Password</div>
  <div class="controls"><%= f.password_field :password_confirmation,:class=>"required" %></div>

  <div><%= f.submit "Sign up" , :class=>"btn btn-primary"%></div>
</fieldset>
<% end %>

<%= render "devise/shared/links" %>
</div>