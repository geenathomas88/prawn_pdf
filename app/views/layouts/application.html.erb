<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= content_for?(:title) ? yield(:title) : "Prawn PDF Example" %></title>
    <meta name="description" content="">
    <meta name="author" content="">
    
    <%= stylesheet_link_tag    "application", :media => "all" %>   
    <%= javascript_include_tag "application" %>
    <%= csrf_meta_tags %>
    <%= yield(:head) %>    
  </head>
  <body>
    
    <div id="main" role="main">
     <%= render 'layouts/messages' %>
    <% if user_signed_in? %>
      <div align="right" class="welcome">
        Welcome, <b><%= current_user.associate_name%></b>&nbsp;|
        <%= link_to('Home', dashboards_path) %>&nbsp;|
        <%= link_to('Logout', destroy_user_session_path, :method=>'delete') %>
      </div>
    <%end%>
    <% if user_signed_in? %>
      <header class="navbar navbar-fixed">
        <nav class="navbar-inner">
          <div class="container">
            <%= render 'layouts/navigation' %>          
          </div>
        </nav>      
      </header>
   <% end %>
   <div class="content">
      <div class="container-fluid">        
           <div class="row-fluid">                        
            <div class="span12">             
              <%= yield %>
            </div>
          </div>
        </div>
      </div> <!--! end of .container -->
      <footer>      
        Copyright Â©2012. Cognizant All Right Reserved      
    </footer>
    </div> <!--! end of #main -->      
  </body>
</html>
